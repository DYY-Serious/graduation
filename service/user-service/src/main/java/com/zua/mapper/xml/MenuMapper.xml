<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zua.mapper.MenuMapper">
    <select id="getLimits" resultType="limit" parameterType="string">
        select lm.* from limits lm where lm.id in (select rl.limitID from role_limit rl where rl.roleID = #{roleID})
    </select>
</mapper>